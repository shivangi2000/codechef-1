#include<stdio.h>
void  main()
{
int n, m,ar1[20] ,ar2[20],i , j, k,ar3[20];
printf(“enter size of array 1 and array 2”);
scanf(“%d %d”,&n,&m);
printf(“enter elements of set A”);
for(int i=0;i<n;i++)
{
  scanf("%d",&ar1[i]);
}
printf(“enter elements of set B”);
                       for(int j=0;j<m;j++)
{
scanf("%d",&ar2[j]);
}
i=0;j=0;k=0;
while(i<n)
{
while(j<m)
{
If(ar1[i]<ar2[j])
{
ar3[k]=ar1[i];
i++; k++;
}
else if(ar1[i]>ar2[j])
{
ar3[k]=ar2[j];
j++; k++;
}
else if(ar1[i]==ar2[j])
{ ar3[k]=ar1[i];
i++; j++; k++;
} }
ar3[k]=ar1[i];
k++; i++;
}
printf(“union of set is”);
for(i=0;i<k;i++)
{
printf("%d ",ar3[n]);
}
}
